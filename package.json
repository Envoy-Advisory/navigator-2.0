
  {
    "name": "fair-chance-navigator-fullstack",
  "version": "1.0.0",
  "description": "Full-stack Fair Chance Navigator 2.0 application",
  "private": true,
  "scripts": {
    "install:all": "npm install && cd client && npm install && cd ../server && npm install",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:local": "concurrently \"npm run dev:server:local\" \"npm run dev:client:local\"",
    "dev:dev": "concurrently \"npm run dev:server:dev\" \"npm run dev:client:dev\"",
    "dev:uat": "concurrently \"npm run dev:server:uat\" \"npm run dev:client:uat\"",
    "dev:server": "cd server && npm run dev",
    "dev:server:local": "cd server && npm run dev:local",
    "dev:server:dev": "cd server && npm run dev:dev",
    "dev:server:uat": "cd server && npm run dev:uat",
    "dev:client": "cd client && npm run dev",
    "dev:client:local": "cd client && npm run dev:local",
    "dev:client:dev": "cd client && npm run dev:dev",
    "dev:client:uat": "cd client && npm run dev:uat",
    "build": "cd server && npm run build && cd ../client && npm run build",
    "build:local": "cd server && npm run build && cd ../client && npm run build:local",
    "build:dev": "cd server && npm run build && cd ../client && npm run build:dev",
    "build:uat": "cd server && npm run build && cd ../client && npm run build:uat",
    "build:prod": "cd server && npm run build:prod && cd ../client && npm run build:prod",
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:local": "concurrently \"npm run start:server:local\" \"npm run start:client:local\"",
    "start:dev": "concurrently \"npm run start:server:dev\" \"npm run start:client:dev\"",
    "start:uat": "concurrently \"npm run start:server:uat\" \"npm run start:client:uat\"",
    "start:prod": "concurrently \"npm run start:server:prod\" \"npm run start:client:prod\"",
    "start:server": "cd server && npm start",
    "start:server:local": "cd server && npm run start:local",
    "start:server:dev": "cd server && npm run start:dev",
    "start:server:uat": "cd server && npm run start:uat",
    "start:server:prod": "cd server && npm run start:prod",
    "start:client": "cd client && npm run preview",
    "start:client:local": "cd client && npm run preview:local",
    "start:client:dev": "cd client && npm run preview:dev",
    "start:client:uat": "cd client && npm run preview:uat",
    "start:client:prod": "cd client && npm run preview:prod",
    "db:generate": "cd server && npm run db:generate",
    "db:push": "cd server && npm run db:push",
    "db:migrate": "cd server && npm run db:migrate",
    "db:reset": "cd server && npm run db:reset",
    "db:seed": "cd server && npm run db:seed"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.1.0",
    "@types/pg": "^8.15.5",
    "concurrently": "^7.6.0"
  }
}
