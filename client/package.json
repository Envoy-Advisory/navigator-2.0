{
  "name": "react-typescript",
  "version": "1.0.0",
  "type": "module",
  "description": "React TypeScript on Replit, using Vite bundler",
  "scripts": {
    "dev": "vite",
    "dev:local": "vite --mode local_dev",
    "dev:dev": "vite --mode dev",
    "dev:uat": "vite --mode uat",
    "build": "tsc && vite build",
    "build:local": "tsc && vite build --mode local_dev",
    "build:dev": "tsc && vite build --mode dev",
    "build:uat": "tsc && vite build --mode uat",
    "build:prod": "tsc && vite build --mode prod",
    "preview": "vite preview",
    "preview:local": "vite preview --mode local_dev",
    "preview:dev": "vite preview --mode dev",
    "preview:uat": "vite preview --mode uat",
    "preview:prod": "vite preview --mode prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.7.1",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^2.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.4.5",
    "typescript": "^4.9.5",
    "vite": "^3.0.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.15.4",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^10.0.0",
    "firebase": "^11.10.0",
    "nodemon": "^3.1.10",
    "react-router-dom": "^7.6.3",
    "ts-node": "^10.9.2",
    "uuid": "^11.1.0"
  }
}
